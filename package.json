{
  "name": "palaemon",
  "version": "1.0.0",
  "description": "- A gentle, euthanization and diagnosis tool for out-of-memory (OOM) kubernetes pods.  - Palaemon is a Greek, childe sea-god who came to aid sailors in distress. He was often depicted as riding a dolphin. Also, a genus of shrimp.",
  "scripts": {
    "build": "webpack --watch --config ./webpack.config.js",
    "build:production": "webpack --config ./webpack.production.js",
    "dev": "concurrently \"npm run build\" \"electronmon ./dist/electron/main.js\"",
    "start": "concurrently \"webpack --watch --config ./webpack.config.js\" \"wait-on ./dist/electron/main.js && electronmon ./dist/electron/main.js\"",
    "start:production": "npm run build:production && electron ./dist/electron/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/Palaemon.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oslabs-beta/Palaemon/issues"
  },
  "homepage": "https://github.com/oslabs-beta/Palaemon#readme",
  "devDependencies": {
    "@kubernetes/client-node": "^0.17.0",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "chart.js": "^3.9.1",
    "child_process": "^1.0.2",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "^20.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.2",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "node-fetch": "^2.2.10",
    "postcss-loader": "^7.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "wait-on": "^6.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  },
  "electronmon": {
    "patterns": [
      "dist/**/*",
      "!client/**/*",
      "!electron/**/*"
    ]
  },
  "dependencies": {
    "module-replace-webpack-plugin": "^0.0.12"
  }
}
